clear      lock
clear   control
clear      mod1
clear      mod2
clear      mod3
clear      mod4
clear      mod5
keycode 166 = Hyper_R
keycode      37 = Hyper_L
keycode 200 = Hyper_L
keycode 66 = Control_L Caps_Lock NoSymbol NoSymbol
add     control = Control_L Control_R
add        mod1 = Alt_L Alt_R Meta_L
add        mod2 = Num_Lock
add        mod3 = Hyper_L Hyper_R
add        mod4 = Super_L Super_R
add        mod5 = Mode_switch
